<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="softwaretestingmaterial">
    <test name="testngTest">
        <classes>
            <class name="packageName.ClassName" />
        </classes>
    </test>
</suite>

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="ExampleSuite" >
    <test name="SuiteTests" >
        <classes>
            <class name="suiteExample.DemoOne"/>
            <class name="suiteExample.DemoTwo"/>
        </classes>
    </test>
</suite>

<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="FSTTests" thread-count="4" parallel="classes"> <!-- Test Suite -->
    <groups> <!-- Groups Control -->
        <run>
            <include name="HeaderTests" />
        </run>
    </groups>
    <test name="FSTActivities"> <!-- Test Classes -->
        <classes>
            <class name="activities.Activity5" />
            <class name="activities.Activity4_DemoOne" />
            <class name="activities.Activity4_DemoTwo" />
        </classes>
    </test>
</suite>


<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="FSTTests">
    <parameter name="username" value="admin" />
    <parameter name="password" value="password" />
    <test name="FSTActivities">
        <classes>
            <class name="activities.Activity6" />
        </classes>
    </test>
</suite>

<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="FSTTests" thread-count="4" parallel="classes">
    <!-- Comment out groups to run everything -->
	<groups>
        <run>
            <include name="HeaderTests" />
        </run>
    </groups>
    <parameter name="username" value="admin" />
    <parameter name="password" value="password" />
    <test name="FSTActivities">
        <classes>
            <class name="activities.Activity1" />
            <class name="activities.Activity2" />
            <class name="activities.Activity3" />
            <class name="activities.Activity4_DemoOne" />
            <class name="activities.Activity4_DemoTwo" />
            <class name="activities.Activity5" />
            <class name="activities.Activity6" />
            <class name="activities.Activity7" />
        </classes>
    </test>
</suite>